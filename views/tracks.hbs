<main class="cards">
    {{#each tracks}}
    <article class="card">
        <div class="text">
            <b>{{this.name}}</b>
            {{#if this.preview_url}}
            <span id="track">
                <audio controls>
                    <source src="{{this.preview_url}}" type="audio/mpeg" />
                </audio>
            </span>
            {{/if}}
        </div>
    </article>
    {{/each}}
</main>